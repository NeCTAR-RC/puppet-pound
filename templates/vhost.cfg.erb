#################### VHOST <%= name %> #######################################

<%=listen_protocol%>
    Address 0.0.0.0
    Port <%= port %>

    <% if listen_protocol == "ListenHTTPS"%>
    Cert "<%= ssl_combined_path %>"
    <% end %>

    ## allow PUT and DELETE also (by default only GET, POST and HEAD)?:
    xHTTP	    1

    Service
        BackEnd
            Address <%= backend_ip %>
            Port <%= backend_port %>
        End
        <% if emergency_ip %>
        Emergency
	    Address <%= emergency_ip %>
	    Port <%= emergency_port %>
        End
        <% end %>  
    End
End
